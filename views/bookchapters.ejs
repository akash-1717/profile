<%- include("partials/header"); -%>

<div class="container mainPadding myFont1">

  <h1>Book chapters</h1>
  <hr class="divider">

  

  <div class="row mb-2">
    <% for (let i = 0; i < data.length; i++) { %>
    <div class="col-md-12">
      <div class="row g-0 project  border overflow-auto flex-md-row mb-4  shadow-lg position-relative publications">
        <div class="col p-4 d-flex flex-column position-static">
            <h5 class="mb-0 thisColor myFont1"><%= data[i].bookchapter %></h5>
            <br>
            <strong class="d-inline-block mb-2 thisColor1">Year: <%= data[i].year %></strong>
            <strong class="d-inline-block mb-2 thisColor1">Book series: <%= data[i].bookseries %></strong>
            <strong class="d-inline-block mb-2 thisColor1">Volume name: <%= data[i].volumename %></strong>
            <strong class="d-inline-block mb-2 thisColor1">Volume number: <%= data[i].volumenumber %></strong>
            <strong class="d-inline-block mb-2 thisColor1">Pages: <%= data[i].pages %></strong>
            <strong class="d-inline-block mb-2 thisColor1">ISBN of series: <%= data[i].isbnseries %></strong>
            <strong class="d-inline-block mb-2 thisColor1">ISBN of volume: <%= data[i].isbnvolume %></strong>
            <strong class="d-inline-block mb-2 thisColor1">Editors of volume: <%= data[i].editors %></strong>
            <strong class="d-inline-block mb-2 thisColor1">Executive Editor: <%= data[i].executiveeditor %></strong>
            <strong class="d-inline-block mb-2 thisColor1">Publisher: <%= data[i].publisher %></strong>
            <strong class="d-inline-block mb-2 thisColor1">Authors: <%= data[i].authors %></strong>
          <br>
        </div>
      </div>
    </div>
    <% } %>
  </div>

</div>

<%- include("partials/footer"); -%>