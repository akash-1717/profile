<%- include("partials/header"); -%>

<div class="form-container">
  <form method="POST" action="/researchprojectsform" style="margin-top: 9rem;">
    <div class="form-group">
      <label for="">Title</label>
      <input type="text" id="" name="title" placeholder="Enter title">
    </div>
    <div class="form-group">
      <label for="">Amount</label>
      <input type="text" id="" name="amount" placeholder="Enter amount">
    </div>
    <div class="form-group">
      <label for="">Year</label>
      <input type="text" id="" name="year" placeholder="Enter year">
    </div>
    <div class="form-group">
      <label for="">Role</label>
      <input type="text" id="" name="role" placeholder="Enter role">
    </div>
    <div class="form-group">
      <label for="">Funding Agency</label>
      <input type="text" id="" name="funding" placeholder="Enter funding agency">
    </div>
    
    <div class="form-group">
      <input type="submit" value="Submit">
    </div>
  </form>
</div>

<%- include("partials/footer"); -%>