<%- include("partials/header"); -%>

<div class="container mainPadding myFont1">

  <h1>Patents</h1>
  <hr class="divider">

  

  <div class="row mb-2">
    <% for (let i = 0; i < data.length; i++) { %>
    <div class="col-md-12">
      <div class="row g-0 project  border overflow-auto flex-md-row mb-4  shadow-lg position-relative publications">
        <div class="col p-4 d-flex flex-column position-static">
          <strong class="d-inline-block mb-2 thisColor1">Patent no.: <%= data[i].pno %></strong>
          <strong class="d-inline-block mb-2 thisColor1">Granted date: <%= data[i].gdate %></strong>
          <h5 class="mb-0 thisColor myFont1"><%= data[i].name %></h5>
          <br>
          <div class="mb-1 text-muted">Contributors: <%= data[i].contributors %></div>
          <div class="mb-1 text-dark">
            <%= data[i].descr %>
          </div>
        </div>
      </div>
    </div>
    <% } %>
  </div>

</div>

<%- include("partials/footer"); -%>