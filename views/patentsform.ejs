<%- include("partials/header"); -%>

<div class="form-container">
  <form method="POST" action="/patentsform" style="margin-top: 9rem;">
    <div class="form-group">
      <label for="title">Patent No.</label>
      <input type="text" id="" name="pno" placeholder="Enter patent number">
    </div>
    <div class="form-group">
      <label for="journal">Granted Date</label>
      <input type="text" id="" name="gdate" placeholder="Enter granted date">
    </div>
    <div class="form-group">
      <label for="year">Name</label>
      <input type="text" id="" name="name" placeholder="Enter patent name">
    </div>
    <div class="form-group">
      <label for="authors">Contributors</label>
      <input type="text" id="" name="contributors" placeholder="Enter contributors">
    </div>
    <div class="form-group">
      <label for="doi">Description</label>
      <input type="text" id="" name="descr" placeholder="Enter description">
    </div>
    <div class="form-group">
      <input type="submit" value="Submit">
    </div>
  </form>
</div>

<%- include("partials/footer"); -%>